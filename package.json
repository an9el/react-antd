{
  "name": "vite-tm-v2",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "postinstall": "simple-git-hooks",
    "format": "biome format --write .",
    "lint": "biome lint . && eslint .",
    "check": "biome check",
    "gcapi": "openapi-generator-cli generate && npm run format",
    "ci:add": "changeset add",
    "ci:_version": "changeset version",
    "ci:_tag": "changeset tag && git push --follow-tags",
    "ci:release": "npm run ci:_version && npm run ci:_tag"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "node scripts/verifyCommit.mjs $1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@ant-design/pro-components": "^2.8.2",
    "@ebay/nice-modal-react": "^1.2.13",
    "@emotion/styled": "^11.13.0",
    "@formily/antd-v5": "1.2.3",
    "@formily/core": "^2.3.2",
    "@formily/react": "^2.3.2",
    "@tanstack/react-query": "^5.60.5",
    "@tanstack/react-router": "^1.82.1",
    "antd": "^5.22.1",
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.27.0",
    "immer": "^10.1.1",
    "jotai": "^2.10.2",
    "jotai-immer": "^0.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.9",
    "@eslint/js": "^9.15.0",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@tanstack/router-devtools": "^1.82.1",
    "@tanstack/router-plugin": "^1.81.9",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-config-sync-biome": "^1.9.4",
    "eslint-plugin-import-x": "^4.4.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lint-staged": "^15.2.10",
    "picocolors": "^1.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11"
  }
}
