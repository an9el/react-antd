{
  "name": "vite-tm-v2",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "postinstall": "simple-git-hooks",
    "format": "biome format --write .",
    "lint": "biome lint . && eslint .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "gcapi": "openapi-generator-cli generate && npm run format",
    "i18n": "i18next-scanner --config scanner.config.cjs",
    "ci:add": "changeset add",
    "ci:_version": "changeset version",
    "ci:_tag": "changeset tag && git push --follow-tags",
    "ci:release": "npm run ci:_version && npm run ci:_tag"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "node scripts/verifyCommit.mjs $1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["biome lint .", "bash -c tsc"],
    "*.{js,jsx,ts,tsx,cjs,mjs}": ["eslint ."],
    "*": ["biome format ."]
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.6",
    "@ant-design/pro-components": "^2.7.1",
    "@auth-kit/react-router": "^3.1.3",
    "@ebay/nice-modal-react": "^1.2.13",
    "@emotion/styled": "^11.11.5",
    "@formily/antd-v5": "1.2.0",
    "@formily/core": "^2.3.1",
    "@formily/react": "^2.3.1",
    "@tanstack/react-query": "^5.32.0",
    "antd": "^5.16.5",
    "dayjs": "^1.11.11",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "immer": "^10.1.1",
    "jotai": "^2.8.0",
    "jotai-immer": "^0.3.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-auth-kit": "^3.1.3",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.1",
    "@changesets/cli": "^2.27.1",
    "@eslint/js": "^9.1.1",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/ui": "^1.5.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.1.1",
    "eslint-config-sync-biome": "^1.7.5",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "i18next-scanner": "^4.4.0",
    "lint-staged": "^15.2.2",
    "picocolors": "^1.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1",
    "vite": "^5.2.10",
    "vitest": "^1.5.2"
  }
}
