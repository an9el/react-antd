{
  "name": "vite-tm-v2",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "postinstall": "simple-git-hooks",
    "format": "prettier --write --cache .",
    "lint": "eslint --cache --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "gcapi": "openapi-generator-cli generate && npm run format",
    "i18n": "i18next-scanner --config scanner.config.cjs",
    "ci:add": "changeset add",
    "ci:_bump": "changeset version",
    "ci:_tag": "git add . && git commit -m \"release: ${npm_package_version}\" && changeset tag && git push --follow-tags",
    "ci:release": "npm run ci:_bump && npm run ci:_tag"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "node scripts/verifyCommit.mjs $1"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{js,jsx,ts,tsx,cjs,mjs}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@ant-design/pro-components": "^2.6.13",
    "@ebay/nice-modal-react": "^1.2.10",
    "@emotion/styled": "^11.11.0",
    "@formily/antd-v5": "1.1.1",
    "@formily/core": "^2.2.29",
    "@formily/react": "^2.2.29",
    "@tanstack/react-query": "^4.33.0",
    "antd": "^5.8.4",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "i18next": "^23.4.5",
    "i18next-http-backend": "^2.2.1",
    "immer": "^10.0.2",
    "jotai": "^2.3.1",
    "jotai-immer": "^0.2.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-auth-kit": "^2.12.6",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.0",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.5.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/ui": "^0.34.2",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "i18next-scanner": "^4.3.0",
    "lint-staged": "^14.0.1",
    "picocolors": "^1.0.0",
    "prettier": "^3.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "simple-git-hooks": "^2.9.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.2"
  }
}
