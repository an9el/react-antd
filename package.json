{
  "name": "vite-tm-v2",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "postinstall": "simple-git-hooks",
    "format": "biome format --write .",
    "lint": "biome lint . && eslint .",
    "check": "biome check",
    "gcapi": "sh scripts/openapi-generate.sh && npm run format",
    "ci:add": "changeset add",
    "ci:_version": "changeset version",
    "ci:_tag": "changeset tag && git push --follow-tags",
    "ci:release": "npm run ci:_version && npm run ci:_tag"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "node scripts/verifyCommit.mjs $1"
  },
  "packageManager": "pnpm@9.12.2",
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "@ant-design/pro-components": "^2.8.2",
    "@ebay/nice-modal-react": "^1.2.13",
    "@formily/antd-v5": "1.2.3",
    "@formily/core": "^2.3.2",
    "@formily/react": "^2.3.2",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-router": "^1.87.7",
    "antd": "^5.22.4",
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.29.0",
    "immer": "^10.1.1",
    "jotai": "^2.10.3",
    "jotai-immer": "^0.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "type-fest": "^4.30.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.10",
    "@eslint/js": "^9.16.0",
    "@tanstack/router-devtools": "^1.87.7",
    "@tanstack/router-plugin": "^1.87.7",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-config-sync-biome": "^1.9.4",
    "eslint-plugin-import-x": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lint-staged": "^15.2.11",
    "picocolors": "^1.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.3"
  }
}
